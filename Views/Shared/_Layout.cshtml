    <div class="cart-dropdown" id="cart-dropdown">
        <div class="cart-content-wrap">
            @if(@cartitems !=null){
            <div class="cart-header">
                <h2 class="header-title">Cart review</h2>
               
                <button class="cart-close sidebar-close"><i class="fas fa-times"></i></button>
                    
                        </div>
            <div class="cart-body">
                <ul class="cart-item-list">
                    @foreach(Product prod in cartitems){
                    <li class="cart-item">
                        <div class="item-img">
                            <a href="@Url.Action("productview", "Shopping", prod)"><img src="@prod.img" alt="@prod.product_name"></a>
                               @using (Html.BeginForm("RemoveFromCart", "Shopping", new  { product_code = prod.product_code }))  
                        {  
                            <button class="close-btn"><i class="fas fa-times"></i></button>
                        }
                        </div>
                        <div class="item-content">
                            <div class="product-rating">
                                <span class="icon">
								<i class="fas fa-star"></i>
								<i class="fas fa-star"></i>
								<i class="fas fa-star"></i>
								<i class="fas fa-star"></i>
								<i class="fas fa-star"></i>
							</span>
                                <span class="rating-number"></span>
                            </div>
                            <h3 class="item-title"><a href="@Url.Action("productview", "Shopping", prod)">@prod.product_name</a></h3>
                            <div class="item-price"><span class="currency-symbol">R</span>@prod.price</div>
                            <div class="pro-qty item-quantity">
                                <input type="number" class="quantity-input" value="@prod.quantity">
                            </div>
                        </div>
                    </li>
                    }
                   
                </ul>
            </div>
            <div class="cart-footer">
                <h3 class="cart-subtotal">
                    <span class="subtotal-amount">R @carttot</span>
                     <span class="subtotal-title">SubTotal:</span>
                   
                </h3>
                <div class="group-btn">
                    <a href="@Url.Action("Cart", "Shopping")" class="axil-btn btn-bg-primary viewcart-btn">View Cart</a>
                    <a href="@Url.Action("CheckOut", "Shopping")" class="axil-btn btn-bg-secondary checkout-btn">Checkout</a>
                </div>
            </div>
            }else{
                <div class="cart-header">
                <h2 class="header-title">Cart review</h2>
                <button class="cart-close sidebar-close"><i class="fas fa-times"></i></button>
                </div>
            <div class="cart-body">
                 <h3 class="cart-subtotal">
                    <span class="subtotal-title">No Items in Cart</span>
                </h3>
            </div>

            <div class="cart-footer">
               
                <div class="group-btn">
                    <a href="@Url.Action("Index", "Shopping")" class="axil-btn btn-bg-primary viewcart-btn">Continue Shopping</a>
                </div>
            </div>
            }
        </div>
