<script type="text/jscript">  
    function SearchProducts(){
        var ser = document.getElementById("prodsearch").val();
        $('#searchresults').load('/Shopping/SearchProducts?prodsearch='+ser);  
    }

     $("#prodsearch").keydown(function () {

      if (event.which == 13) { 


         event.preventDefault();
            var searchvalue = $("#prodsearch").val();
            
            if(searchvalue !=''){
            $.ajax({
                type: 'GET',
                url: '/Shopping/SearchProducts',
                data: {
                    prodsearch: searchvalue
                }
            })

            .done(function (response) {
                $("#searchresults").html(response);
            })

            .fail(function (XMLHttpRequest, textStatus, errorThrown) {
                alert("Fail");
            });
       }
       }
        });
